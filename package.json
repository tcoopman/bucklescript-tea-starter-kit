    "start": "run-s watch"
  },
  "keywords": [
    "Bucklescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "4.0.6",
    "bucklescript-tea": "0.8.0",
    "npm-run-all": "4.1.3",
    "rollup": "0.66.6",
    "rollup-plugin-livereload": "0.6.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "serve": "10.0.2"
  }
}
